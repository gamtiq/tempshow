{
  "name": "tempshow",
  "version": "1.0.0",
  "description": "React component to temporary show some components/elements",
  "source": "src/TempShow.js",
  "main": "dist/tempshow.js",
  "module": "dist/tempshow.es.js",
  "umd:main": "dist/tempshow.umd.js",
  "scripts": {
    "build": "microbundle --jsx React.createElement --no-compress",
    "lint": "eslint --cache --max-warnings 0 \"**/*.js\"",
    "lint-error": "eslint --cache \"**/*.js\"",
    "lint-all": "eslint --max-warnings 0 \"**/*.js\"",
    "lint-all-error": "eslint \"**/*.js\"",
    "check": "npm run lint && npm test",
    "check-all": "npm run lint-all && npm test",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamtiq/tempshow.git"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "temporary",
    "temp",
    "show",
    "hide",
    "element",
    "elements",
    "children",
    "components",
    "ui"
  ],
  "peerDependencies": {
    "prop-types": ">=15.5.4",
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "author": "Denis Sikuler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gamtiq/tempshow/issues"
  },
  "homepage": "https://github.com/gamtiq/tempshow#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "chai": "4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.9.0",
    "eslint-config-guard": "^2.0.1",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.3.0",
    "microbundle": "^0.12.3",
    "mocha": "^8.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-util": "1.0.0"
  },
  "dependencies": {}
}
